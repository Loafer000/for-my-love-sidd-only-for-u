{
  "name": "connectspace-backend",
  "version": "1.0.0",
  "description": "ConnectSpace rental platform backend API",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest --coverage --watchAll=false",
    "test:watch": "jest --watch",
    "test:unit": "jest --testPathPattern=unit",
    "test:integration": "jest --testPathPattern=integration",
    "test:api": "jest --testPathPattern=api",
    "test:coverage": "nyc --reporter=html --reporter=text jest",
    "test:load": "artillery run tests/load/load-test.yml",
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --ext .js --fix",
    "lint:ci": "eslint . --ext .js --config .eslintrc.ci.json",
    "security:audit": "npm audit",
    "security:check": "npm audit --audit-level moderate",
    "build": "node build.js"
  },
  "keywords": [
    "rental",
    "property",
    "real-estate",
    "api"
  ],
  "author": "ConnectSpace Team",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cloudinary": "^1.40.0",
    "compression": "^1.7.4",
    "connect-mongo": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.10.0",
    "express-session": "^1.18.2",
    "express-validator": "^7.0.1",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^7.5.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^7.0.9",
    "razorpay": "^2.9.2",
    "twilio": "^4.15.0",
    "validator": "^13.15.15"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/plugin-transform-runtime": "^7.28.3",
    "@babel/preset-env": "^7.28.3",
    "@types/jest": "^29.5.5",
    "artillery": "^2.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.48.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-security": "^1.7.1",
    "jest": "^29.6.4",
    "mongodb-memory-server": "^8.15.1",
    "nodemon": "^3.0.1",
    "nyc": "^15.1.0",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
